{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["node_modules/**"]
      }
    }
  ],
  "build": {
    "command": "npm install"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
